type Character {
    id: ID! @unique
    createdAt: DateTime!
    name: String!
    description: String!
}

type Place {
    id: ID! @unique
    createdAt: DateTime!
    name: String!
    description: String!
}

type Occurrence {
    id: ID! @unique
    createdAt: DateTime!
    name: String!
    description: String!
    startDate: DateTime!
    endDate: DateTime
    characters: [Character!]!
    places: [Place!]!
}
